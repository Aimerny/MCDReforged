msgid "Behavior"
msgstr "表现"

#: ../cli.rst:215 ../cli.rst:216 ../cli.rst:218 ../cli.rst:222
msgid "Unset"
msgstr "未设置"

#: ../cli.rst:217
msgid "Read the ignore list from .gitignore"
msgstr "从 .gitignore 文件中读取忽略列表"

#: ../cli.rst:219 ../cli.rst:221 ../cli.rst:224 ../cli.rst:225
msgid "Set"
msgstr "已设置"

#: ../cli.rst:220
msgid "Read the ignore list from given file"
msgstr "从给定的文件中读取忽略列表"

